<Window x:Class="IVSwitcher.licence"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:IVSwitcher"
        xmlns:properties="clr-namespace:IVSwitcher.Properties"
        mc:Ignorable="d"
        Title="{x:Static properties:Resources.settings_oss}" Height="450" Width="800">
    <Grid>
        <Grid.RowDefinitions>
            <RowDefinition Height="Auto"/>
            <RowDefinition Height="Auto"/>
        </Grid.RowDefinitions>
        <TextBox
            Grid.Row="0"
            Text="{x:Static properties:Resources.settings_OSS_LICENCE}"
            VerticalScrollBarVisibility="Visible"
            IsReadOnly="True"
            />
        <TextBlock Grid.Row="1">
            <Hyperlink NavigateUri="https://icons8.com" RequestNavigate="Hyperlink_RequestNavigate">
                <TextBlock Text="{x:Static properties:Resources.startup_cfg_icon}"/>
            </Hyperlink>
        </TextBlock>
        
    </Grid>
</Window>
