<Window x:Class="IVSwitcher.licence"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:IVSwitcher"
        xmlns:properties="clr-namespace:IVSwitcher.Properties"
        mc:Ignorable="d"
        Title="{x:Static properties:Resources.settings_oss}" Height="450" Width="800">
    <Grid>
        <TextBox
            Text="{x:Static properties:Resources.settings_OSS_LICENCE}"
            VerticalScrollBarVisibility="Visible"
            IsReadOnly="True"
            />

    </Grid>
</Window>
